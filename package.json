{
  "name": "kit-global-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 5000",
    "clear": "rm -rf .next",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "trans:compile": "formatjs compile src/i18n/translations/en.json --out-file src/i18n/locales/defaultMessages.json && rm src/i18n/translations/en.json",
    "trans:extract": "formatjs extract 'src/**/*.ts*' --out-file src/i18n/translations/en.json --id-interpolation-pattern '[folder][name][sha512:contenthash:base64:6]'",
    "trans": "yarn trans:extract && yarn trans:compile",
    "trans:merge": "yarn trans && node ./src/i18n/mergeLocales.js --lang en --path ./src/i18n/locales --new-translation newTranslation",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-react": "^7.16.7",
    "@codica/eslint-config-react-ts": "^2.1.1",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@formatjs/cli": "^4.8.4",
    "@formatjs/ts-transformer": "^3.11.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.71",
    "@mui/material": "^5.6.4",
    "@mui/system": "^5.6.4",
    "@mui/x-date-pickers": "^5.0.3",
    "@next/bundle-analyzer": "^13.2.4",
    "@types/axios": "^0.14.0",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "axios": "^1.3.4",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "husky": "^8.0.3",
    "next": "13.2.4",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "next-react-svg": "^1.2.0",
    "next-redux-wrapper": "^8.1.0",
    "next-sitemap": "^4.0.6",
    "prettier": "^2.8.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^5.24.8",
    "react-lazy-hydration": "^0.1.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.7",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@types/redux-logger": "^3.0.9",
    "babel-plugin-formatjs": "^10.3.7",
    "redux-logger": "^3.0.6"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint",
      "prettier --write"
    ]
  }
}
